{
  "author": "Danil Semelenov",
  "bin": "./cli.js",
  "description": "Converts npm packages to native ES6 modules which can be imported directly from browsers without bundlers and transpilers.",
  "devDependencies": {
    "eslint": "^4.19.1",
    "find-npm-prefix": "^1.0.2",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "license": "ISC",
  "name": "esmy",
  "repository": "https://github.com/sgtpep/esmy.git",
  "scripts": {
    "format": "prettier --write './**/*.{css,js,json,jsx,md,mjs}'",
    "lint": "eslint --fix './**/*.{js,jsx,mjs}'",
    "prepublishOnly": "rollup -c",
    "test": "prettier -l './**/*.{css,js,json,jsx,md,mjs}' && eslint './**/*.{js,jsx,mjs}' && node ./test.js"
  },
  "version": "1.0.8"
}
