{
  "author": "Danil Semelenov",
  "bin": "./cli.js",
  "description": "Converts npm packages to native ES6 modules which can be imported directly from modern browsers without bundlers and transpilers.",
  "devDependencies": {
    "eslint": "^4.19.1",
    "find-npm-prefix": "^1.0.2",
    "npm-package-arg": "^6.1.0",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "keywords": [
    "browser",
    "es6",
    "esm",
    "import",
    "jspm",
    "module",
    "npm",
    "package",
    "vanilla"
  ],
  "license": "MIT",
  "name": "esmy",
  "repository": "https://github.com/sgtpep/esmy.git",
  "scripts": {
    "format": "prettier --write './**/*.{js,json,md,mjs}'",
    "lint": "eslint --fix './**/*.{js,mjs}'",
    "prepublishOnly": "rollup -c",
    "test": "prettier -l './**/*.{js,json,md,mjs}' && eslint './**/*.{js,mjs}'"
  },
  "version": "1.2.0"
}
